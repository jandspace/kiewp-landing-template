.example-courses {
  width: 100%;
  float: left;
  margin-top: 40px;

  &__title {
    display: block;
    text-transform: uppercase;
    font-weight: 700;
    margin: 0 0 5px 0;
    font-size: 18px;
    color: $darkGray;
  }

  &__subtitle {

    display: block;
    margin-bottom: 20px;
    font-weight: normal;
    font-size: 14px;
    color: $lightGray;

  }


}